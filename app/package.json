{
  "name": "rhizine",
  "productName": "rhizine",
  "version": "0.0.1",
  "description": "Decentralised code collaboration.",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Radicle Team",
    "email": "hello@radicle.xyz"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./native/webpack.main.config.js",
            "renderer": {
              "config": "./native/webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./native/index.html",
                  "js": "./native/renderer.js",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "dependencies": {
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@electron-forge/cli": "6.0.0-beta.45",
    "@electron-forge/plugin-webpack": "6.0.0-beta.45",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "electron": "6.0.12",
    "node-loader": "^0.6.0",
    "react-hot-loader": "^4.12.15",
    "style-loader": "^0.23.1"
  }
}
